{
  "name": "@jpravetz/strava-api",
  "description": "Strava API",
  "version": "2.0.4",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts"
  },
  "tasks": {
    "test": "deno test -A",
    "lint": "deno lint",
    "check": "deno check src/**/*.ts test/**/*.ts"
  },
  "tests": {
    "include": [
      "test/**/*.ts",
      "src/**/*.test.ts"
    ]
  },
  "imports": {
    "@epdoc/datetime": "jsr:@epdoc/datetime@^2.0.3",
    "@epdoc/duration": "jsr:@epdoc/duration@^0.3.3",
    "@epdoc/fs": "jsr:@epdoc/fs@^1.2.0",
    "@epdoc/logger": "jsr:@epdoc/logger@^1003.0.5",
    "@epdoc/msgbuilder": "jsr:@epdoc/msgbuilder@^0.0.7",
    "@epdoc/type": "jsr:@epdoc/type@^1.1.19",
    "@oak/oak": "jsr:@oak/oak@^17.2.0",
    "@opensrc/deno-open": "jsr:@opensrc/deno-open@^1.0.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/testing/bdd": "jsr:@std/testing@^1.0.16/bdd",
    "@std/expect": "jsr:@std/expect@^1.0.17/expect"
  },
  "lint": {
    "include": [
      "src",
      "test"
    ],
    "exclude": [
      "test/tmp",
      "test/readonly"
    ]
  }
}